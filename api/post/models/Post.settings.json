{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "coverArt": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "body": {
      "type": "text"
    },
    "authors": {
      "collection": "user",
      "via": "blogposts",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "categories": {
      "collection": "category",
      "via": "blogposts"
    }
  }
}